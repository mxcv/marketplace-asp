{
  "JwtConfiguration": {
    "Issuer": "marketplace",
    "Audience": "client",
    "AccessLifetime": "900", //15 minutes
    "RefreshLifetime": "2592000", //30 days
    "Key": "super secret key"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=marketplace;Trusted_Connection=True;"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
